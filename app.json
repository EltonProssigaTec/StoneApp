{
  "expo": {
    "name": "StoneUP",
    "slug": "StoneApp",
    "version": "3.4.4",
    "private": true,
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "icon": "./assets/images/ic_launcher.png",
    "scheme": "stoneapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "br.com.stoneup.monitora.app",
      "buildNumber": "3.4.4",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Este app precisa de acesso à câmera para você tirar fotos do perfil.",
        "NSPhotoLibraryUsageDescription": "Este app precisa de acesso à galeria de fotos para você escolher uma foto de perfil.",
        "NSPhotoLibraryAddUsageDescription": "Este app precisa de acesso para salvar fotos na galeria.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "br.com.stoneup.monitora.app",
      "versionCode": 74,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "com.android.vending.BILLING"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Este app precisa de acesso à câmera para você tirar fotos do perfil."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8804226e-b71f-4ed3-92b8-5be85ed2d6aa"
      }
    },
    "owner": "eltonprossiga"
  }
}