{
  "expo": {
    "name": "StoneApp",
    "slug": "StoneApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "stoneapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Este app precisa de acesso à câmera para você tirar fotos do perfil.",
        "NSPhotoLibraryUsageDescription": "Este app precisa de acesso à galeria de fotos para você escolher uma foto de perfil.",
        "NSPhotoLibraryAddUsageDescription": "Este app precisa de acesso para salvar fotos na galeria."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#fff",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ],
      "navigationBar": {
        "visible": "sticky-immersive",
        "barStyle": "light-content"
      },
      "statusBar": {
        "barStyle": "light-content",
        "translucent": true
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.StoneApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8804226e-b71f-4ed3-92b8-5be85ed2d6aa"
      }
    },
    "owner": "eltonprossiga"
  }
}
