{
  "name": "monitora",
  "main": "expo-router/entry",
  "version": "3.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:release": "cd android && ./gradlew assembleRelease && cd ..",
    "android:bundle": "cd android && ./gradlew bundleRelease && cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "expo run:ios",
    "ios:clean": "cd ios && xcodebuild clean && cd ..",
    "ios:pods": "cd ios && pod install && cd ..",
    "web": "expo start --web",
    "lint": "expo lint",
    "run-emulator": "adb kill-server && yarn android",
    "prebuild": "npx expo prebuild --clean",
    "prebuild:ios": "npx expo prebuild --platform ios --clean",
    "clean-gradlew": "cd android && ./gradlew clean",
    "build-debug:apk": "cd android  && ./gradlew assembleDebug",
    "build-release:aab": "cd android  && ./gradlew  bundleRelease",
    "build-release:apk": "cd android  && ./gradlew  assembleRelease ",
    "build-ios:simulator": "npx eas-cli build --platform ios --profile development --local",
    "build-ios:preview": "npx eas-cli build --platform ios --profile preview",
    "build-ios:production": "npx eas-cli build --platform ios --profile production",
    "open-path-from-apk-genereted": "cd android/app/build/ && start .",
    "open-ios-workspace": "open ios/*.xcworkspace",
    "release-aab": "yarn prebuild && yarn clean-gradlew && yarn build-release:aab",
    "release-apk": "yarn prebuild && yarn clean-gradlew && yarn build-release:apk",
    "release-ios": "yarn prebuild:ios && yarn ios:pods && yarn build-ios:production",
    "run-adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "run-adb-reverse-device": "adb -s emulator-5554 reverse tcp:8081 tcp:8081"
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.4.2",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/drawer": "^7.5.9",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "axios": "^1.12.2",
    "expo": "~54.0.10",
    "expo-camera": "~17.0.9",
    "expo-checkbox": "^5.0.7",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-navigation-bar": "^5.0.8",
    "expo-router": "~6.0.8",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "lucide-react": "^0.544.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-unistyles": "^3.0.15",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
